{
  "type": "datasource",
  "name": "VictoriaMetrics",
  "id": "victoriametrics-datasource",
  "category": "tsdb",
  "routes": [
    {
      "method": "POST",
      "path": "api/v1/query",
      "reqRole": "Viewer"
    },
    {
      "method": "POST",
      "path": "api/v1/query_range",
      "reqRole": "Viewer"
    },
    {
      "method": "POST",
      "path": "api/v1/series",
      "reqRole": "Viewer"
    },
    {
      "method": "POST",
      "path": "api/v1/labels",
      "reqRole": "Viewer"
    },
    {
      "method": "GET",
      "path": "/rules",
      "reqRole": "Viewer"
    },
    {
      "method": "POST",
      "path": "/rules",
      "reqRole": "Editor"
    },
    {
      "method": "DELETE",
      "path": "/rules",
      "reqRole": "Editor"
    },
    {
      "method": "DELETE",
      "path": "/config/v1/rules",
      "reqRole": "Editor"
    },
    {
      "method": "POST",
      "path": "/config/v1/rules",
      "reqRole": "Editor"
    }
  ],
  "metrics": true,
  "alerting": true,
  "annotations": true,
  "backend": false,
  "queryOptions": {
    "minInterval": true
  },
  "info": {
    "description": "VictoriaMetrics datasource plugin for Grafana",
    "author": {
      "name": "VictoriaMetrics",
      "url": "https://victoriametrics.com/"
    },
    "keywords": [
      "VictoriaMetrics",
      "datasource"
    ],
    "logos": {
      "small": "img/vm_logo.svg",
      "large": "img/vm_logo.svg"
    },
    "screenshots": [
      {
        "name": "Connection properties",
        "path": "img/datasource.jpg"
      },
      {
        "name": "Query editor",
        "path": "img/query_editor.jpg"
      }
    ],
    "links": [
      {
        "name": "Learn more",
        "url": "https://docs.victoriametrics.com/"
      }
    ],
    "version": "0.1.0",
    "updated": "%TODAY%"
  },
  "dependencies": {
    "grafanaDependency": ">=8.3.0",
    "plugins": []
  }
}
